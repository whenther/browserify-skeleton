{
  "name": "browserify-skeleton",
  "private": false,
  "version": "0.0.0",
  "description": "browserify project skeleton for a meetup talk",
  "contributors": [
    "Will <will@whentheresawill.net>"
  ],
  "keywords": [
    "browserify",
    "angular"
  ],
  "dependencies": {
    "angular": "^1.4.7",
    "angular-ui-router": "^0.2.15",
    "lodash": "^3.10.1"
  },
  "scripts": {},
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Will",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.6.5",
    "browserify": "^12.0.1",
    "browserify-istanbul": "^0.2.1",
    "browserify-shim": "^3.8.11",
    "connect-livereload": "^0.5.3",
    "connect-modrewrite": "^0.8.2",
    "fs": "0.0.2",
    "fs-walk": "0.0.1",
    "grunt": "^0.4.5",
    "grunt-banner": "^0.6.0",
    "grunt-browserify": "^4.0.1",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-coffee": "~0.10.1",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-connect": "^0.11.2",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-cssmin": "~0.9.0",
    "grunt-contrib-imagemin": "^1.0.0",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-contrib-jst": "~0.6.0",
    "grunt-contrib-less": "0.11.1",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-uglify": "~0.4.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-karma": "^0.12.1",
    "grunt-ng-annotate": "^1.0.1",
    "grunt-nodemon": "^0.4.0",
    "grunt-sails-linker": "~0.9.5",
    "grunt-shell": "^1.1.2",
    "grunt-shell-spawn": "^0.3.10",
    "grunt-sync": "~0.0.4",
    "grunt-version": "^1.0.0",
    "include-all": "~0.1.3",
    "jasmine-core": "^2.3.4",
    "jshint-stylish": "^2.1.0",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.2.2",
    "karma-ng-html2js-preprocessor": "^0.2.0",
    "karma-phantomjs2-launcher": "^0.4.0",
    "morgan": "^1.6.1",
    "partialify": "^3.1.5",
    "transformify": "^0.1.2"
  },
  "browser": {
    "angular-local-storage": "./bower_components/angular-local-storage/dist/angular-local-storage.js",
    "angular-hotkeys": "./bower_components/angular-hotkeys/angular-hotkeys.js",
    "angular-ui-mask": "./bower_components/angular-ui-mask/dist/mask.js",
    "sticky-kit": "./bower_components/sticky-kit/jquery.sticky-kit.js",
    "ng-pattern-restrict": "./bower_components/ng-pattern-restrict/src/ng-pattern-restrict.js",
    "angular-percentage-filter": "./bower_components/angular-percentage-directive/percentage.js",
    "typeahead": "./bower_components/typeahead.js/dist/typeahead.jquery.js",
    "froogaloop": "./bower_components/froogaloop/froogaloop.js",
    "angular-translate-loader-partial": "./node_modules/angular-translate/dist/angular-translate-loader-partial/angular-translate-loader-partial.js",
    "angular-loading-bar": "./node_modules/angular-loading-bar/build/loading-bar.min.js",
    "angular-scroll": "./node_modules/angular-scroll/angular-scroll.min.js",
    "angular-translate-storage-local": "./node_modules/angular-translate/dist/angular-translate-storage-local/angular-translate-storage-local.js",
    "liveaddress": "./source/public/js/lib/jquery.liveaddress-2.8.js"
  },
  "browserify-shim": {
    "jquery": "jQuery",
    "google": "window.google",
    "angular-local-storage": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('LocalStorageModule').name"
    },
    "angular-cookies": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('ngCookies').name"
    },
    "angular-ui-notification": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('ui-notification').name"
    },
    "angular-ui-mask": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('ui.mask').name"
    },
    "ng-pattern-restrict": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('ngPatternRestrict').name"
    },
    "angular-hotkeys": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('drahak.hotkeys').name"
    },
    "angular-loading-bar": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('angular-loading-bar').name"
    },
    "angulartics-google-analytics": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angulartics.google.analytics"
    },
    "angular-scroll": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('duScroll').name"
    },
    "angular-percentage-filter": {
      "depends": [
        "angular:angular"
      ],
      "exports": "angular.module('percentage').name"
    },
    "liveaddress": {
      "depends": [
        "jquery:jQuery"
      ],
      "exports": "jQuery.LiveAddress"
    },
    "froogaloop": "window.Froogaloop"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "partialify"
    ]
  }
}
